import json

# لیست آدرس‌های فیشینگ (blacklist نمونه)
blacklist = [
    "TRYwbGCBw8qEFECEnwKH1ZtsQEpvg4SDev",
    "TCPYrSScuHLLPrBAobcHM7X1QBFjLVt4Zz",
    "TRTRW5Z2Q547bFSq4CUmVfSZbVxrumUJ4J"
]

# فایل JSON تو
filename = "phishing-addresses.json"

# باز کردن فایل و بارگذاری JSON
